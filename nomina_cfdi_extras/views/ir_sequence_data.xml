<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        <!-- Sequences for hr.loan -->
        <record id="hr_loan_sequence" model="ir.sequence">
            <field name="name">Secuencia de prestamo</field>
            <field name="code">employee.loan</field>
            <field name="prefix">PRES/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>
        
        <!-- Sequences for Skipp installment -->
        <record id="dev_skip_installment" model="ir.sequence">
            <field name="name">Secuencia de salto de prestamo</field>
            <field name="code">dev.skip.installment</field>
            <field name="prefix">SAL-PRES/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>
    </data>
</odoo>
