<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_dar_baja_form" model="ir.ui.view">
            <field name="name">wizard.dar.baja.form</field>
            <field name="model">wizard.dar.baja</field>
            <field name="arch" type="xml">
                <form string="Baja de empleado">
                <group>
                	<group col="4">
                		<field name="fecha"/>
                	</group>
                    <group col="4">
                        <field name="tipo_de_baja"/>
                    </group>
                </group>
                	<footer>
                        <button name="action_dar_baja" string="Confirmar" type="object" class="oe_highlight"/>
                        <button string="Cancelar" class="oe_highlight" special="cancel" />
                    </footer>
               </form>
            </field>
        </record>
        
    </data>
</odoo>