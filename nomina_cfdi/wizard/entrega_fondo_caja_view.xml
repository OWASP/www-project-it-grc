<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <record id="entrega_fondo_caja_view_form" model="ir.ui.view">
        <field name="name">entrega.fondo.caja.view.form</field>
        <field name="model">entrega.fondo.caja</field>
        <field name="arch" type="xml">
            <form string="Altas y Bajas">
                <group>
                	<group>
                    	<field name="fecha_solicitud" required="1"/>
                    	<field name="fecha_aplicacion" required="1"/>
                    </group>
                    <group>
                    	<field name="descripcion" required="1"/>
                    	<field name="clave" required="1"/>
                    </group>
                </group>
                <footer>
                    <button name="create_caja_de_ahorro" string="Generar" type="object" class="oe_highlight"/>
                    <button string="Cancelar" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>    

    </data>
</odoo>
