
<odoo>
    <data>
        <record model="ir.ui.view" id="mana_dashboard.file_url_list">
            <field name="name">Mana Dashboard File Url list </field>
            <field name="model">mana_dashboard.file_url</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="url" string="url"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="mana_dashboard.file_url_form">
            <field name="name">Mana Dashboard File Url form </field>
            <field name="model">mana_dashboard.file_url</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="form-group row">
                            <label for="url" class="col-sm-2 col-form-label">url</label>
                            <div class="col-sm-10">
                                <field name="url" string="url"/>
                            </div>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="mana_dashboard.file_url_act_window">
            <field name="name">Mana Dashboard File Url</field>
            <field name="res_model">mana_dashboard.file_url</field>
            <field name="view_mode">tree,form</field>
        </record>
        
    </data>
</odoo>