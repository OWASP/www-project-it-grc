
<odoo>
    <data>

        <record model="ir.ui.view" id="mana_dashboard.datetime_range_list">
            <field name="name">Mana Date Time Range list </field>
            <field name="model">mana_dashboard.datetime_range</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="range_type" string="range_type"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="mana_dashboard.datetime_range_form">
            <field name="name">Mana Date Time Range form </field>
            <field name="model">mana_dashboard.datetime_range</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="form-group row">
                            <label for="range_type" class="col-sm-2 col-form-label">range_type</label>
                            <div class="col-sm-10">
                                <field name="range_type" string="range_type"/>
                            </div>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="mana_dashboard.datetime_range_act_window">
            <field name="name">Mana Date Time Range</field>
            <field name="res_model">mana_dashboard.datetime_range</field>
            <field name="view_mode">tree,form</field>
        </record>
        
    </data>
</odoo>