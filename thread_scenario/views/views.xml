<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!--  TaxonomyThreadresource  -->
    <record model="ir.ui.view" id="taxonomy_thread_resource_treeview">
      <field name="name">taxonomy.thread.resource.treeview</field>
      <field name="model">taxonomy.thread.resource</field>
      <field name="arch" type="xml">
        <tree>
          <field name="type_threat_source"/>
          <field name="description"/>
          <field name="characteristics"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="taxonomy_thread_resource_formview">
      <field name="name">taxonomy.thread.resource.formview</field>
      <field name="model">taxonomy.thread.resource</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
                <field name="type_threat_source"/>
                <field name="description"/>
                <field name="characteristics"/>
              </group>
              <group></group>
            </group>
          </sheet>
          <div class="oe_chatter">
            <field name="activity_ids" widget="mail_activity" />
            <field name="message_ids" widget="mail_thread" />
          </div>
        </form>
      </field>
    </record>

