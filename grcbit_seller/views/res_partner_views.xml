<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
    <record id="grcbit_seller_res_partner_inherit_tree" model="ir.ui.view">
      <field name="name">grcbit_seller.res_partner.inherit.tree</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//tree" position="attributes">
          <attribute name="delete">0</attribute>
        </xpath>
        <xpath expr="//field[@name='country_id']" position="after">
          <field name="state"/>
        </xpath>
      </field>
    </record>

    <record id="grcbit_seller_res_partner_inherit_form" model="ir.ui.view">
      <field name="name">grcbit_seller.res_partner.inherit.form</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="arch" type="xml">
        <xpath expr="//form" position="attributes">
          <attribute name="delete">0</attribute>
        </xpath>
        <xpath expr="//field[@name='vat']" position="after">
          <field name="endpoints"/>
        </xpath>
        <xpath expr="//notebook" position="inside">
          <page string="Threat Fend" name="grc_seller" groups="grcbit_seller.group_admin_seller,grcbit_seller.group_reseller_admin">
            <group>
              <field name="is_admin" invisible="1"/>
              <field name="client_system" force_save="1" attrs="{'readonly': [('is_admin','=', False)]}"/>
              <field name="state" attrs="{'readonly': [('is_admin','=', False)]}"/>
              <field name="activate_date" attrs="{'readonly': [('is_admin','=', False)]}"/>
              <field name="is_openziti" attrs="{'readonly': [('is_admin','=', False)]}"/>
            </group>
            <group>
              <group string="Database">
                <field name="db_postgres_port" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="postgres_pwd" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="xdr_pwd" attrs="{'readonly': [('is_admin','=', False)]}"/>
              </group>
              <group string="GRC">
                <field name="grc_web_port" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="dns_domain" force_save="1" attrs="{'readonly': [('is_admin','=', False)]}"/>
              </group>
            </group>
            <group string="XDR">
              <group>
                <field name="xdr_indexer_port" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="xdr_dashboard_port" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="xdr_dashboard_port2" attrs="{'readonly': [('is_admin','=', False)]}"/>
              </group>
              <group>
                <field name="xdr_manager_port1" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="xdr_manager_port2" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="xdr_manager_port3" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="xdr_manager_port4" attrs="{'readonly': [('is_admin','=', False)]}"/>
              </group>
            </group>
            <group string="OPENZITI">
              <group>
                <field name="ziti_controller_port1" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="xiti_controller_port2" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="ziti_edge_router1" attrs="{'readonly': [('is_admin','=', False)]}"/>
              </group>
              <group>
                <field name="ziti_edge_router2" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="ziti_console" attrs="{'readonly': [('is_admin','=', False)]}"/>
                <field name="xdr_zt" attrs="{'readonly': [('is_admin','=', False)]}"/>
              </group>
            </group>
            <group string="Output">
              <group>
                <field name="xdr_index_container" readonly="1"/>
                <field name="xdr_server_container" readonly="1"/>
                <field name="xdr_dash_container" readonly="1"/>
              </group>
              <group>
                <field name="psql_container_id" readonly="1"/>
                <field name="grc_container_id" readonly="1"/>
                <field name="network" readonly="1"/>
                <field name="ztrust_console" readonly="1"/>
                <field name="ztrust_router" readonly="1"/>
                <field name="ztrust_controller" readonly="1"/>
              </group>
            </group>
          </page>
        </xpath>
     
      </field>
    </record>
  </data>
</odoo>